[user]
	name  = isuzuki
	email = 

[core]
	autocrlf = input
	editor   = vim

[color]
	ui          = true
	status      = auto
	diff        = auto
	branch      = auto
	interactive = auto
	grep        = auto

[alias]
	alias = !git config --list | grep 'alias\\.' | sed 's/alias\\.\\([^=]*\\)=\\(.*\\)/\\1\\\t => \\2/' | sort
	br = branch
	ci = commit -va
	co = checkout
	di = diff
	st = status
	delete-merged-br = !git branch --merged | grep -v \\* | xargs -I % git branch -d %
